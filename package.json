{
  "name": "duck-keybindings",
  "displayName": "Duck Keymaps",
  "description": "Some custom keymaps for VS Code. Quack.",
  "version": "3.0.0",
  "publisher": "zackschuster",
  "engines": {
    "vscode": "^1.3.0"
  },
  "categories": [
    "Keymaps"
  ],
  "keywords": [
    "keymap"
  ],
  "preview": false,
  "icon": "duck.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/zackschuster/vscode-duck-keybindings.git"
  },
  "bugs": {
    "url": "https://github.com/zackschuster/vscode-duck-keybindings/issues"
  },
  "contributes": {
    "keybindings": [
      {
        "key": "ctrl+shift+a",
				"mac": "cmd+shift+a",
        "command": "editor.emmet.action.wrapWithAbbreviation"
      },
      {
				"key": "ctrl+shift+s",
				"mac": "cmd+shift+s",
        "command": "workbench.action.files.saveAll"
      },
      {
				"key": "ctrl+,",
				"mac": "cmd+,",
        "command": "workbench.action.focusFirstEditorGroup"
      },
      {
        "key": "shift+f5",
        "command": "workbench.action.reloadWindow",
        "when": "!inDebugMode"
      },
      {
        "key": "f4",
        "command": "workbench.action.tasks.runTask"
      },
      {
        "key": "ctrl+.",
        "mac": "cmd+.",
        "command": "workbench.action.terminal.focus"
      },
      {
				"key": "ctrl+;",
				"mac": "cmd+;",
        "command": "workbench.action.terminal.focusNext"
      },
      {
				"key": "ctrl+shift+;",
				"mac": "cmd+shift+;",
        "command": "workbench.action.terminal.focusPrevious"
      },
      {
        "key": "ctrl+shift+/",
				"mac": "cmd+shift+/",
        "command": "workbench.action.terminal.kill"
      },
      {
        "key": "ctrl+shift+'",
				"mac": "cmd+shift+'",
        "command": "workbench.action.terminal.new"
      },
      {
        "key": "ctrl+'",
        "mac": "cmd+'",
        "command": "workbench.action.terminal.toggleTerminal"
      }
    ]
  }
}
